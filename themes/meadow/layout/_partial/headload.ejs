<%# Google Adsense Auto Ads %>
<%if(theme.google_adsense.enabled){%>
    <%if(theme.google_adsense.auto_ad){%>
        <script data-ad-client="ca-<%= theme.google_adsense.pub_id %>" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <%}else{%>
        <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <%}%>
<%}%>
<%# Google Analytics %>
<%if(theme.analytics.google_analytics.tracking_id){%>
    <%- partial('_plugin/analytics/google-analytics') %>
<%}%>
<%# Baidu Analytics %>
<%if(theme.analytics.baidu_site_id){%>
    <%- partial('_plugin/analytics/baidu-analytics') %>
<%}%>
<%# CNZZ Analytics %>
<%if(theme.analytics.cnzz_site_id){%>
    <%- partial('_plugin/analytics/cnzz-analytics') %>
<%}%>
<%# Tencent Analytics %>
<%if(theme.analytics.tencent_site_id){%>
    <%- partial('_plugin/analytics/tencent-analytics') %>
<%}%>

<%# Math %>
<%if(theme.math.enabled == true){%>
    <%if (is_home() || (page.mathjax==true) || (page.katex==true)) {%>
        <%- partial('_plugin/math/' + theme.math.engine) %>
    <%}%>      
<%}%>
